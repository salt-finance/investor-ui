{
  "name": "investor-ui",
  "version": "0.1.1",
  "type": "module",
  "description": "Investor UI is the investor portal built on svelte",
  "scripts": {
    "dev": "npm-run-all --parallel vite build:tailwind-dev",
    "vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:tailwind": "tailwind build -i index.css -o public/tailwind.css --minify",
    "build:tailwind-dev": "tailwind build -i index.css -o public/tailwind.css --watch",
    "install:clear": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install && npm run build:tailwind && npm run build:fontawesome && npm run dev",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "svelte": "^5.2.7",
    "svelte-check": "^4.1.0",
    "tailwindcss": "^3.4.16",
    "tslib": "^2.8.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "npm-run-all": "*"
  },
  "dependencies": {
    "@kurkle/color": "^0.3.4",
    "@popperjs/core": "2.11.8",
    "@tailwindcss/forms": "0.5.9",
    "chart.js": "4.4.7",
    "svelte-spa-router": "4.0.1"
  }
}
